<div class="container body-content">


    <h1>@ViewBag.title</h1>


    <ul>

        @foreach (var whatever in ViewBag.jobs)
        {

            <li>ID: @whatever.Id</li>
            <li>Name: @whatever.Name</li>
            <li>Employer: @whatever.Employer</li>
            <li>Location: @whatever.Location</li>
            <li>Position Type: @whatever.CoreCompetency</li>

        }

    </ul>

</div>